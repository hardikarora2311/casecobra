
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Configuration{
  id        String @id @default(cuid())
  width      Int
  height     Int
  imageUrl   String
  croppedImgUrl String?
}